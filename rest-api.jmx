        <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="JSR223 PreProcessor" enabled="true">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">import org.apache.jmeter.engine.util.CompoundVariable;
import org.apache.jmeter.protocol.http.util.HTTPArgument;

// Set the input in HTTP request body, if it exists
if(!vars.get(&quot;input&quot;).equals(&quot;&quot;)) {
    def input = new CompoundVariable(new File(vars.get(&quot;project.root&quot;) + vars.get(&quot;input&quot;)).text).execute();
    def arg = new HTTPArgument(&quot;&quot;, input, null, true);
    arg.setAlwaysEncoded(false);
    sampler.getArguments().addArgument(arg);

    // Set the HTTP body
    log.info(input);
}</stringProp>
        </JSR223PreProcessor>
